@model IEnumerable<InternalsApplication.Models.Subject>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Subject_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Subject_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Subject_Code }) |
            @Html.ActionLink("Details", "Details", new { id=item.Subject_Code }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Subject_Code })
        </td>
    </tr>
}

</table>
